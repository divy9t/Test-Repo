<odoo>
	<data>
		<record id="sn_tree_view" model="ir.ui.view">
			<field name="name">Pravasi Config</field>
			<field name="model">school_net.school_net</field>
			<field name="arch" type="xml">
				<tree string="Pravasi Config">
					<field name="name"/>
				</tree>
			</field>
		</record>


		<record id="sn_action" model="ir.actions.act_window">
			<field name="name">Pravasi Config</field>
			<field name="res_model">school_net.school_net</field>
			<field name="view_mode">tree</field>
			<field name="domain">[]</field>
			<field name="help" type="html">
			   <p class="o_view_nocontent_smiling_face">Welcome To Pravasi Config</p>
			</field>
		</record>
		
		<menuitem id="sn_master_menu"
            name="Pravasi Config"
            action="sn_action"/>
	</data>
</odoo>